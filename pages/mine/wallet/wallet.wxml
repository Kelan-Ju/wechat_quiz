<!--pages/wallet/wallet.wxml-->
<view class='mine-bg'>
  <image class='mine-bg-image' src='http://39.106.179.254//public//uploads//b04a3e1fd9ff4ea3efd1160b73a010bc.png'></image>
  <!-- 返回按钮 -->
  <view class='back-view' bindtap='backClick'>
    <image class='back' src='http://39.106.179.254//public//uploads//19eb70b1a923922213ce5505c74770b1.png'></image>
  </view>
  <view class='name'>我的钱包</view>
  <view class='content-view'>
    <!-- 金额 -->
    <view class='money-view'>
      <view class='money'>23.4</view>
      <text class='money-title'>我的金额</text>
    </view>
    <!-- 使用方式 -->
    <view class='use-list'>
      <view class='item' bindtap='itemClick' data-index='0'>
        <image class='item-icon' src='http://39.106.179.254//public//uploads//a60cd5c3dd3f57d915e71c96e963156b.png'></image>
        <view class='item-title'>提现</view>
        <image src='http://39.106.179.254//public//uploads//818e55cd7fe9d924f5003c10f6f57ead.png' class='enter'></image>
      </view>
      <view class='item02' bindtap='itemClick' data-index='1'>
        <image class='item-icon' src='http://39.106.179.254//public//uploads//490b0de1b9ac6ad9294a4bcd284079bd.png'></image>
        <view class='item-title'>兑换奖品</view>
        <image src='http://39.106.179.254//public//uploads//818e55cd7fe9d924f5003c10f6f57ead.png' class='enter'></image>
      </view>
    </view>
  </view>
</view>