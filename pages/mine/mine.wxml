<!--pages/mine/mine.wxml-->
<view class='mine-bg'>
  <image class='mine-bg-image' src='http://39.106.179.254//public//uploads//b04a3e1fd9ff4ea3efd1160b73a010bc.png'></image>
  <!-- 返回按钮 -->
  <view class='back-view' bindtap='backClick'>
    <image class='back' src='http://39.106.179.254//public//uploads//19eb70b1a923922213ce5505c74770b1.png'></image>
  </view>
  <view class='name'>个人中心</view>
  <view class='content-view'>
    <!-- 头像 -->
    <view class='head-view'>
      <open-data type="userAvatarUrl" class='head'></open-data>
      <open-data type="userNickName" class='userName'></open-data>
    </view>
    <!-- 列表 -->
    <view class='content-list'>
      <view class='item' bindtap='itemClick' data-index='0'>
        <image class='item-icon' src='http://39.106.179.254//public//uploads//79b8b252abf0640642a767e2faaed01a.png'></image>
        <view class='item-title'>我的钱包</view>
        <image src='http://39.106.179.254//public//uploads//818e55cd7fe9d924f5003c10f6f57ead.png' class='enter'></image>
      </view>
      <view class='item' bindtap='itemClick' data-index='1'>
        <image class='item-icon' src='http://39.106.179.254//public//uploads//724a4dcddf0c8922ed014a4488ade0de.png'></image>
        <view class='item-title'>得利排行</view>
        <image src='http://39.106.179.254//public//uploads//818e55cd7fe9d924f5003c10f6f57ead.png' class='enter'></image>
      </view>
      <view class='item' bindtap='itemClick' data-index='2'>
        <image class='item-icon' src='http://39.106.179.254//public//uploads//4563c26f4aeb884a4fe05983cd21f68e.png'></image>
        <view class='item-title'>我的订单</view>
        <image src='http://39.106.179.254//public//uploads//818e55cd7fe9d924f5003c10f6f57ead.png' class='enter'></image>
      </view>
      <view class='item02' bindtap='itemClick' data-index='3'>
        <image class='item-icon' src='http://39.106.179.254//public//uploads//1cedb1adbddac4d524f910428b0ef975.png'></image>
        <view class='item-title'>收货地址</view>
        <image src='http://39.106.179.254//public//uploads//818e55cd7fe9d924f5003c10f6f57ead.png' class='enter'></image>
      </view>
    </view>
  </view>
</view>